{
    "margo": {
        "argobots": {
            "pools": [
                {
                    "name": "rpc_pool",
                    "kind": "fifo_wait",
                    "access": "mpmc"
                }
            ]
        },
        "rpc_pool": "rpc_pool"
    },
    "libraries": {
        "flock": "libflock-bedrock-module.so",
        "yokan": "libyokan-bedrock-module.so"
    },
    "providers": [
        {
            "name": "service_group",
            "type": "flock",
            "provider_id": 1,
            "pool": "__primary__",
            "config": {
                "bootstrap": "mpi",
                "group": {
                    "type": "static",
                    "config": {}
                },
                "file": "distributed_service.flock"
            }
        },
        {
            "name": "kv_store",
            "type": "yokan",
            "provider_id": 42,
            "pool": "rpc_pool",
            "config": {
                "database": {
                    "type": "map"
                }
            },
            "dependencies": {
                "group": "service_group"
            }
        }
    ]
}
