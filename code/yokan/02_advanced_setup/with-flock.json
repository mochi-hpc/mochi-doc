{
    "libraries": {
        "flock": "libflock-bedrock-module.so",
        "yokan": "libyokan-bedrock-module.so"
    },
    "providers": [
        {
            "name": "yokan_group",
            "type": "flock",
            "provider_id": 1,
            "pool": "__primary__",
            "config": {
                "bootstrap": "mpi",
                "group": {
                    "type": "static",
                    "config": {}
                },
                "file": "yokan_group.flock"
            }
        },
        {
            "name": "my_yokan_db",
            "type": "yokan",
            "provider_id": 42,
            "pool": "__primary__",
            "config": {
                "database": {
                    "type": "rocksdb",
                    "config": {
                        "path": "/data/yokan_db",
                        "create_if_missing": true
                    }
                }
            },
            "dependencies": {
                "group": "yokan_group"
            }
        }
    ]
}
